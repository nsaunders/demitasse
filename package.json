{
  "name": "demitasse",
  "description": "Zero-runtime CSS-in-TS",
  "version": "1.0.1",
  "scripts": {
    "format": "prettier --write *.src.js extract.js *.ts test.js",
    "prepublishOnly": "terser index.src.js -o index.js && terser common.src.js -o common.js",
    "test": "terser index.src.js -o index.js && terser common.src.js -o common.js && node ./test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nsaunders/demitasse.git"
  },
  "keywords": [
    "vanilla",
    "emotion",
    "style",
    "css-in-js",
    "css-in-ts",
    "css",
    "typescript"
  ],
  "dependencies": {
    "csstype": "^3.0.8"
  },
  "devDependencies": {
    "prettier": "^2.3.2",
    "terser": "^5.7.1"
  }
}
